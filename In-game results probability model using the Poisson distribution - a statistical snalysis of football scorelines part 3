    "    draw_prob = model('Arsenal','Watford',2,0,i)[1]\n",
    "    watford_prob = model('Arsenal','Watford',2,0,i)[2]\n",
    "    \n",
    "    arsenal_list.append(arsenal_prob)\n",
    "    draw_list.append(draw_prob)\n",
    "    watford_list.append(watford_prob)    \n",
