    "def plot_model(gd):\n",
    "    plot_list = []\n",
    "    for t in x:\n",
    "        plot = model(t, gd)[0]\n",
    "        \n",
    "        plot_list.append(plot)\n",
    "       \n",
    "    return(plot_list)"
   ]
